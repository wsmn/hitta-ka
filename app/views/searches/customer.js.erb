var customerTable = document.querySelector("#customers-table");
if (customerTable != null) {
  customerTable.innerHTML = "<%= escape_javascript(render('/customers/table', customers: @customers, pagy: @pagy, search: true)) %>";
  if (Alpine != undefined) {
    Alpine.discoverUninitializedComponents(el => {
      Alpine.initializeComponent(el);
    }, customerTable);
  }
} else {
  console.log("Could not find the tag");
}
